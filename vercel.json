{
    "version": 2,
    "builds": [
      {
        "src": "studybud/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize":"15mb", "runtime": "Python 3.10.4"}
      },
      {
        "src": "build_file.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_build"
        }

      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        
      },
      {
        "src": "/(.*)",
        "dest": "studybud/wsgi.py"
       
      }
    ]
}