{
    
    "builds": [
      {
        "src": "studybud/wsgi.py",
        "use": "@vercel/python",
        "config": {"maxLambdaSize":"15mb", "runtime": "Python 3.9"}
      }
      
    ],
    "routes": [
      
      {
        "src": "/(.*)",
        "dest": "studybud/wsgi.py"
       
      }
    ]
}